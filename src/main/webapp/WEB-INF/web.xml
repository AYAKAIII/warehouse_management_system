<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <display-name>Archetype Created Web Application</display-name>
  <welcome-file-list>
    <welcome-file>/login.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>com.service.Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>

    <servlet>
        <servlet-name>comList</servlet-name>
        <servlet-class>com.service.ComList</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>comList</servlet-name>
        <url-pattern>/comList</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>addCom</servlet-name>
        <servlet-class>com.service.AddCom</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>addCom</servlet-name>
        <url-pattern>/addCom</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>delCom</servlet-name>
        <servlet-class>com.service.DelCom</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>delCom</servlet-name>
        <url-pattern>/delCom</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>updateCom</servlet-name>
        <servlet-class>com.service.UpdateCom</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>updateCom</servlet-name>
        <url-pattern>/updateCom</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>selCom</servlet-name>
        <servlet-class>com.service.SelCom</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>selCom</servlet-name>
        <url-pattern>/selCom</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>setEx</servlet-name>
        <servlet-class>com.service.SetEx</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>setEx</servlet-name>
        <url-pattern>/setEx</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>getEx</servlet-name>
        <servlet-class>com.service.GetEx</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getEx</servlet-name>
        <url-pattern>/getEx</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>userList</servlet-name>
        <servlet-class>com.service.UserList</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>userList</servlet-name>
        <url-pattern>/userList</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>addUser</servlet-name>
        <servlet-class>com.service.AddUser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>addUser</servlet-name>
        <url-pattern>/addUser</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>delUser</servlet-name>
        <servlet-class>com.service.DelUser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>delUser</servlet-name>
        <url-pattern>/delUser</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>updateUser</servlet-name>
        <servlet-class>com.service.UpdateUser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>updateUser</servlet-name>
        <url-pattern>/updateUser</url-pattern>
    </servlet-mapping>




<servlet-mapping>
    <servlet-name>default</servlet-name>
    <url-pattern>*.css</url-pattern>
    <url-pattern>*.js</url-pattern>
    <url-pattern>*.gif</url-pattern>
    <url-pattern>*.jpg</url-pattern>
    <url-pattern>*.jpeg</url-pattern>
    <url-pattern>*.png</url-pattern>
    <url-pattern>*.ico</url-pattern>
    <url-pattern>*.zip</url-pattern>
    <url-pattern>*.rar</url-pattern>
    <url-pattern>*.json</url-pattern>
    <url-pattern>*.woff2</url-pattern>
    <url-pattern>*.html</url-pattern>
  </servlet-mapping>
  </web-app>
